{
  "compilerOptions": {
    "lib": ["es2023"],
    "target": "es2023",
    "moduleResolution": "node",
    "module": "CommonJS",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "checkJs": false
  }
}
